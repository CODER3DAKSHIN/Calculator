<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/frame_31__1_"
    android:orientation="vertical"
    tools:context=".MainActivity">
    <EditText
        android:id="@+id/tf1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:layout_marginBottom="10dp"
        android:maxLines="1"
        android:marqueeRepeatLimit="marquee_forever"
        android:textSize="50dp"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        <ImageButton
            android:id="@+id/tAc"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/frame_22"
            android:onClick="removeall"
            />
        <ImageButton
            android:id="@+id/tb"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/group_1"
            android:onClick="remove"
            />
        <ImageButton
            android:id="@+id/td"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/group_2"
            android:onClick="add"
            />
        <ImageButton
            android:id="@+id/tm"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/group_3"
            android:onClick="add"
            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        <ImageButton
            android:id="@+id/t7"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/frame_9"
            android:onClick="add"
            />
        <ImageButton
            android:id="@+id/t8"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/frame_11"
            android:onClick="add"
            />
        <ImageButton
            android:id="@+id/t9"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/frame_13"
            android:onClick="add"
            />
        <ImageButton
            android:id="@+id/ts"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/frame_8"
            android:onClick="add"
            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
        <ImageButton
            android:id="@+id/t4"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
            android:background="@drawable/frame_5"
            android:onClick="add"
            /> <ImageButton
            android:id="@+id/t5"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
        android:background="@drawable/frame_6"
            android:onClick="add"
        /> <ImageButton
            android:id="@+id/t6"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="10dp"
        android:background="@drawable/frame_7"
            android:onClick="add"
        />
        </LinearLayout>
        <ImageButton
            android:id="@+id/tp"
            android:layout_height="110dp"
            android:layout_width="80dp"
            android:layout_marginTop="-100dp"
            android:layout_marginStart="305dp"
        android:background="@drawable/frame_24"
            android:onClick="add"
        />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
        android:orientation="horizontal">

            <ImageButton
                android:id="@+id/t1"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/frame_1"
                android:onClick="add"/>

        <ImageButton
            android:id="@+id/t2"
            android:layout_height="110dp"
            android:layout_width="110dp"
            android:layout_marginTop="-14dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/frame_2"
            android:onClick="add"/>
        <ImageButton
            android:id="@+id/t3"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_margin="-10dp"
            android:background="@drawable/frame_3"
            android:onClick="add"
            />

        </LinearLayout>
            <ImageButton
                android:id="@+id/teq"
                android:layout_height="240dp"
                android:layout_width="130dp"
                android:layout_marginTop="-95dp"
                android:layout_marginStart="270dp"
        android:background="@drawable/frame_20"
                android:onClick="result"
        />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="-140dp"
            android:layout_marginStart="10dp"
            >
        <ImageButton
            android:id="@+id/t0"
            android:layout_height="80dp"
            android:layout_width="180dp"
            android:background="@drawable/frame_23"
            android:onClick="add"
            />
        <ImageButton
            android:id="@+id/t00"
            android:layout_height="80dp"
            android:layout_width="80dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/group_8"
            android:onClick="add"
            />
        </LinearLayout>




    </LinearLayout>
</LinearLayout>